/* pages/index/daili.wxss */
page{
  width: 100%;
  height: 100%;
}
.daili{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.daili image{
  position: fixed;
  top: 0;
  left:0;
  right: 0;
  bottom:0;
  width:100%;
  height:100%;
  z-index: -1;
}
.f-item{
  display: flex;
  align-items: center;
  border: 1px solid #0B8031;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
  padding:15rpx 10rpx;
  font-size: 30rpx;
  color: #0B8031;
  border-radius: 5px;
}
.btn-area button{
  background-color: #0B8031;
  color: #fff;
}
.yi{
  background-color: #fff;
  margin: 100rpx;
  text-align: center;
  padding:100rpx 50rpx;
  border: 1px solid #0B8031;
  border-radius: 5px;
}
.yi text{
  display: block;
  font-size: 30rpx;
  color: #0B8031;
}
