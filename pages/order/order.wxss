page{
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
}
.no-login{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  width: 100%;
  height: calc(100% - 134rpx);
}
.no-login .tou{
  width: 119rpx;
  height: 119rpx;
  margin-bottom: 15rpx;
}
.no-login view{
  color: #0B8031;
  font-size: 30rpx;
}
.no-login .bottom{
  width: 100%;
  height: 134rpx;
  position: fixed;
  bottom: 0;
  left: 0;
}
.o-nav{
  display: flex;
  align-items: center;
  background-color: #fff;
}
.o-nav text{
  flex: 1;
  text-align: center;
  color: #828282;
  font-size: 30rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.o-active{
  color: #0B8031 !important;
}
.item{
  margin-top: 20rpx;
}
.i-top{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  font-size: 24rpx;
  color: #828282;
  padding: 21rpx;
}
.i-red{
  color: #F20A30;
}
.i-middle{
  display: flex;
  align-items: center;
  padding: 21rpx;
}
.i-middle image{
  width: 230rpx;
  height: 195rpx;
  margin-right: 20rpx;
}
.i-m-right text{
  display: block;
  font-size: 30rpx;
  color: #333;
}
.i-m-right text:last-child{
  color: #F20A30;
  padding-top: 80rpx;
}
.i-bottom{
  text-align: right;
  background-color: #fff;
  font-size: 24rpx;
  padding: 21rpx;
}
.i-bottom text{
  box-sizing: border-box;
  padding: 10rpx 30rpx;
  display: inline-block;
  border-radius: 5px;
}
.pay{
  color: #0B8031;
  border: 1px solid #0B8031;
  margin-right: 10rpx;
}
.check{
  color: #fff;
  background-color: #0B8031;
  border: 1px solid #0B8031;
}